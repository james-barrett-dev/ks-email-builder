<table class="kiddybank" cellpadding="0" cellspacing="0" border="0" width="100%">
  <tbody>
    <tr>
      <td>
        {{#> block-content-container props='background white' }}
          <table cellpadding="0" cellspacing="0" border="0" width="100%">
            <tr>
              <td class="kiddybank_scalloped-edge top" valign="bottom">
                <!--[if gte mso 9]>
                <v:rect xmlns:v="urn:schemas-microsoft-com:vml" fill="tile" stroke="false" style="width: 640px; height: 8px;">
                  <v:fill type="tile" src="../src/images/components/kiddybank/kiddybank_scalloped-edge_top_x1.png"/>
                  <v:textbox inset="0,0,0,0">
                <![endif]-->
                <!--[if gte mso 9]>
                  </v:textbox>
                </v:rect>
                <![endif]-->
              </td>
            </tr>
          </table>
        {{/block-content-container}}
        {{#> block-content-container props='padded background white-ice' }}
          <table class="kiddybank" border="0" cellpadding="0" cellspacing="0" width="100%">
            <tr>
              <td class="kiddybank_inner">
                <table border="0" cellpadding="0" cellspacing="0" width="100%">
                  <tr>
                    <td class="kiddybank_heading" align="center">
                      <h2 class="fontFamily fontSize headline titleMobile arial colour paradiso">
                        <span class="fontFamily fontSize headline titleMobile barlow">
                          KIDDYBANK SUMMARY
                        </span>
                      </h2>
                    </td>
                  </tr>
                  <tr>
                    <td>
                      <table border="0" cellpadding="0" cellspacing="0" width="100%">
                        <tr>
                          <th class="column" width="33%">
                            <table border="0" cellpadding="0" cellspacing="0" width="100%">
                              <tr>
                                <td class="kiddybank_pod_wrapper first">
                                  <table border="0" cellpadding="0" cellspacing="0" width="100%">
                                    <tr>
                                      <td class="kiddybank_pod">
                                        <table border="0" cellpadding="0" cellspacing="0" width="100%">
                                          <tr>
                                            <td class="kiddybank_pod_title" align="center">
                                              <h3 class="fontFamily arial">
                                                <span class="fontFamily barlow">
                                                  In Kiddybank
                                                </span>
                                              </h3>
                                            </td>
                                          </tr>
                                          <tr>
                                            <td class="kiddybank_pod_sum" align="center">
                                              <p class="fontFamily arial">
                                                <span class="fontFamily bariol">
                                                  £{recipient_x_1}
                                                </span>
                                              </p>
                                            </td>
                                          </tr>
                                        </table>
                                      </td>
                                    </tr>
                                  </table>
                                </td>
                              </tr>
                            </table>
                          </th>
                          <th class="column" width="33%">
                            <table border="0" cellpadding="0" cellspacing="0" width="100%">
                              <tr>
                                <td class="kiddybank_pod_wrapper second">
                                  <table border="0" cellpadding="0" cellspacing="0" width="100%">
                                    <tr>
                                      <td class="kiddybank_pod">
                                        <table border="0" cellpadding="0" cellspacing="0" width="100%">
                                          <tr>
                                            <td class="kiddybank_pod_title" align="center">
                                              <h3 class="fontFamily arial">
                                                <span class="fontFamily barlow">
                                                  Total pending
                                                </span>
                                              </h3>
                                            </td>
                                          </tr>
                                          <tr>
                                            <td class="kiddybank_pod_sum" align="center">
                                              <p class="fontFamily arial">
                                                <span class="fontFamily bariol">
                                                  £{recipient_x_4}
                                                </span>
                                              </p>
                                            </td>
                                          </tr>
                                        </table>
                                      </td>
                                    </tr>
                                  </table>
                                </td>
                              </tr>
                            </table>
                          </th>
                          <th class="column" width="33%">
                            <table border="0" cellpadding="0" cellspacing="0" width="100%">
                              <tr>
                                <td class="kiddybank_pod_wrapper third">
                                  <table border="0" cellpadding="0" cellspacing="0" width="100%">
                                    <tr>
                                      <td class="kiddybank_pod">
                                        <table border="0" cellpadding="0" cellspacing="0" width="100%">
                                          <tr>
                                            <td class="kiddybank_pod_title" align="center">
                                              <h3 class="fontFamily arial">
                                                <span class="fontFamily barlow">
                                                  Transferred out
                                                </span>
                                              </h3>
                                            </td>
                                          </tr>
                                          <tr>
                                            <td class="kiddybank_pod_sum" align="center">
                                              <p class="fontFamily arial">
                                                <span class="fontFamily bariol">
                                                  £{recipient_x_2}
                                                </span>
                                              </p>
                                            </td>
                                          </tr>
                                        </table>
                                      </td>
                                    </tr>
                                  </table>
                                </td>
                              </tr>
                            </table>
                          </th>
                        </tr>
                      </table>
                    </td>
                  </tr>
                  <tr>
                    <td>
                      <table border="0" cellpadding="0" cellspacing="0" width="100%">
                        <tr>
                          <td class="kiddybank_pod_wrapper fourth" align="center">
                            <table border="0" cellpadding="0" cellspacing="0" align="center" width="100%">
                              <tr>
                                <td class="kiddybank_pod">
                                  <table border="0" cellpadding="0" cellspacing="0" width="100%">
                                    <tr>
                                      <td class="kiddybank_pod_title" align="center">
                                        <h3 class="fontFamily arial">
                                          <span class="fontFamily barlow">
                                            Total saved
                                          </span>
                                        </h3>
                                      </td>
                                    </tr>
                                    <tr>
                                      <td class="kiddybank_pod_sum" align="center">
                                        <p class="fontFamily arial">
                                          <span class="fontFamily bariol">
                                            £{recipient_x_3}
                                          </span>
                                        </p>
                                      </td>
                                    </tr>
                                  </table>
                                </td>
                              </tr>
                            </table>
                          </td>
                        </tr>
                      </table>
                    </td>
                  </tr>
                </table>
              </td>
            </tr>
          </table>
        {{/block-content-container}}
        {{#> block-content-container props='background white' }}
          <table cellpadding="0" cellspacing="0" border="0" width="100%">
            <tr>
              <td class="kiddybank_scalloped-edge bottom" valign="top">
                <!--[if gte mso 9]>
                <v:rect xmlns:v="urn:schemas-microsoft-com:vml" fill="tile" stroke="false" style="width: 640px; height: 8px;">
                  <v:fill type="tile" src="../src/images/components/kiddybank/kiddybank_scalloped-edge_bottom_x1.png"/>
                  <v:textbox style="mso-fit-shape-to-text:true" inset="0,0,0,0">
                <![endif]-->
                <!--[if gte mso 9]>
                  </v:textbox>
                </v:rect>
                <![endif]-->
              </td>
            </tr>
          </table>
        {{/block-content-container}}
      </td>
    </tr>
  </tbody>
</table>